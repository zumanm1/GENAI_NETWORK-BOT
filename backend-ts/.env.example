# Server Configuration
HOST=0.0.0.0
PORT=5000
NODE_ENV=development
SECRET_KEY=your-secret-key-here

# Database (for future use)
DATABASE_URL=sqlite:///network_whisperer.db

# LLM Provider API Keys
GROQ_API_KEY=your-groq-api-key-here
OPENAI_API_KEY=your-openai-api-key-here
CLAUDE_API_KEY=your-claude-api-key-here

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# Network Device Default Credentials
DEFAULT_SSH_USERNAME=admin
DEFAULT_SSH_PASSWORD=your-default-password
DEFAULT_ENABLE_PASSWORD=your-enable-password

# Security Settings
SSH_TIMEOUT=30
MAX_CONCURRENT_CONNECTIONS=10

# Logging
LOG_LEVEL=info
LOG_FILE=network_whisperer.log

# CORS Settings
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# JWT Settings
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=24h
